<ivy-module version="1.0">

	<info organisation="de.pellepelster.myadmin" module="generator" status="integration" />

	<configurations>
		<conf name="generated.client.web" />
		<conf name="generated.client.mobile" />
		<conf name="generated.client.base" />
		<conf name="generated.xml" />
		<conf name="generated.server" extends="generated.client.web, generated.client.base, generated.xml, generated.client.mobile" />
		<conf name="master" />
		<conf name="runtime" />
		<conf name="sources" />
		<conf name="compile" extends="runtime" />
 		<conf name="test" extends="compile" />
		<conf name="default" extends="master, runtime" />
    </configurations>
    
	<publications>
		<artifact name="generator" type="jar" ext="jar" conf="master" />
		<artifact name="myadmin-mobile-client-gen" type="jar" ext="jar" conf="generated.client.mobile" />
		<artifact name="myadmin-web-client-gen" type="jar" ext="jar" conf="generated.client.web" />
		<artifact name="myadmin-client-base-gen" type="jar" ext="jar" conf="generated.client.web, generated.client.mobile" />
		<artifact name="myadmin-server-gen" type="jar" ext="jar" conf="generated.server" />
		<artifact name="myadmin-xml-gen" type="jar" ext="jar" conf="generated.xml" />
	</publications>

	<dependencies defaultconf="compile->default">

		<dependency name="dsl-generator" rev="latest.integration" conf="*->master;compile->compile" />
		<dependency name="client-base" rev="latest.integration" conf="*->master" />

		<dependency name="server-core" rev="latest.integration" conf="*->master" />
		<dependency name="db" rev="latest.integration" conf="*->master" />

		<dependency org="ant" name="ant" rev="1.6.5" >
			<artifact name="ant" type="jar" />
		</dependency>

		<dependency org="com.google.inject" name="guice" rev="3.0" />
		<dependency org="com.sun.jaxb" name="annox" rev="0.5.1" />
		<dependency org="com.sun.jaxb" name="jaxb-api" rev="2.6.5" />
		<dependency org="com.sun.jaxb" name="jaxb-impl" rev="2.6.5" />
		<dependency org="com.sun.jaxb" name="jaxb-xjc-javadoc" rev="2.6.5" />
		<dependency org="com.sun.jaxb" name="jaxb-xjc" rev="2.6.5" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics-annotate" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics-ant" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics-runtime" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics-testing" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics-tools" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jaxb2-basics" rev="0.6.3" />
		<dependency org="com.sun.jaxb" name="jsr173" rev="api_1.0" />
 		
	</dependencies>

</ivy-module>
