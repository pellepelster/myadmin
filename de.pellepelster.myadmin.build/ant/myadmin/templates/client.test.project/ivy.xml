<ivy-module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="@organisation.name@" module="@project.name@.client.test" status="integration"></info>

	<configurations>
		<conf name="master" />
		<conf name="runtime" />
		<conf name="sources" />
		<conf name="compile" extends="default" />
		<conf name="test" extends="compile" />
		<conf name="default" extends="master, runtime" />
	</configurations>

	<publications>
	</publications>

	<dependencies defaultconf="*->default">

		<dependency org="@organisation.name@" name="@project.name@.client" rev="latest.integration" conf="*->@" />

		<dependency org="de.pellepelster.myadmin" name="@project.name@.generator" rev="latest.integration" conf="*->generated.client.web" />

		<dependency org="de.pellepelster.myadmin" name="gwt-test" rev="latest.integration" conf="*->default,sources" />

	</dependencies>

</ivy-module>
